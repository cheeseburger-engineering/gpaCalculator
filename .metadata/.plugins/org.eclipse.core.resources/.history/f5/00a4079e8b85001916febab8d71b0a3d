package org.cheeseburger.gpacalculator.beans;

public class Grade {

	private String code;
	private String name;
	private float gpa;

	public Grade() {

	}

	public Grade(String code, String name, float gpa) {
		this.code = code;
		this.name = name;
		this.gpa = gpa;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public float getGPA() {
		return GPA;
	}

	public void setGPA(float gpa) {
		this.gpa = gpa;
	}

}