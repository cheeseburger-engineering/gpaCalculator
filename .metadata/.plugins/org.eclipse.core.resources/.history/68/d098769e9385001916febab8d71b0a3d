package org.cheeseburger.gpacalculator.beans;

public class Grade {

	private String code;
	private String name;
	private float gpa;

	public Grade() {

	}

	public Grade(String code, String name, float gpa) {
		setCode(code);
		setName(name);
		setGpa(gpa);
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public float getGpa() {
		return gpa;
	}

	public void setGpa(float gpa) {
		this.gpa = gpa;
	}

}