package org.cheeseburger.gpacalculator.beans;

public class Grade {

	private String code;
	private String name;
	private float price;

	public Grade() {

	}

	public Grade(String code, String name, float price) {
		this.code = code;
		this.name = name;
		this.price = price;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public float getPrice() {
		return price;
	}

	public void setGpa(float price) {
		this.price = price;
	}

}